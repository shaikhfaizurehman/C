void main(){//increment operator
//30 aug 2022
#include <stdio.h>

void main(){
    //dicrement operater 
    int i;
    i=15;
    printf("value of i=%d\n",i);
    i--;
    printf("value of i=%d\n",i);
    printf("*********\n");
    //postincrement
    int  x;
    x=i++;
    printf("value of i=%d\n",i);
    printf("value of x=%d",x);
    
    
}
